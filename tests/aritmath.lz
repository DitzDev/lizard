fnc add(int a, int b ) -> int {
   return a + b 
}

println(add(30, 2000))

# minus operation
fnc minus(int a, int b ) -> int {
   return a - b 
}

println(minus(30, 20))

# times operation
fnc times(int a, int b) -> int {
   return a * b
}

println(times(300, 800))

# Division operation
fnc division(int a, int b ) -> float {
   return a / b
}

println(division(100, 23))

# Division return type int operation
fnc div_int(int a, int b) -> int {
   return a %% b
}

println(div_int(100, 23))

# modulo operation
fnc modulo(int a, int b) {
   return a % b
}
println(modulo(2, 100))